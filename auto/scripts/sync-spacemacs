#!/usr/bin/env bash
SOURCE_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

${SOURCE_DIR}/sync-git-repo \
  --repo https://github.com/syl20bnr/spacemacs.git \
  --branch develop \
  --remote-branch origin/develop \
  --ref 48ffd37594825b2a2f3e6ff09e18460618449da9 \
  --target-dir "$HOME/.emacs.d"
