#!/usr/bin/env bash
SOURCE_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

SYNC_NAME="aws-shortcuts" \
    SYNC_REPO=git@git.realestate.com.au:jake-woods/aws-shortcuts.git \
    SYNC_REF=57163d0a94664629feaae8c58f7cd93a36016598 \
    SYNC_TARGET_DIR=$HOME/.aws-shortcuts \
    ${SOURCE_DIR}/../sync-git-repo
